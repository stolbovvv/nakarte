{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "warn"
	},
	"overrides": [
		{
			"files": ["gulpfile.js"],
			"env": {
				"node": true
			}
		}
	]
}
